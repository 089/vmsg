{
  "name": "vmsg",
  "version": "0.0.0",
  "description": "Library for creating voice messages",
  "main": "vmsg.js",
  "scripts": {
    "start": "parcel demo/index.html",
    "demo": "D=`mktemp -d` && parcel build demo/index.html --out-dir \"$D\" --public-url ./ && git checkout gh-pages && rm -f *.{html,js,css,wasm} && mv \"$D\"/* . && rmdir \"$D\" && git add -A && git commit -m 'Update demo'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kagami/vmsg.git"
  },
  "keywords": [
    "voice",
    "voice message",
    "emscripten",
    "webassembly",
    "lame",
    "mp3"
  ],
  "author": "Kagami Hiiragi",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/Kagami/vmsg/issues"
  },
  "homepage": "https://github.com/Kagami/vmsg#readme",
  "parcelDisableLoaders": [
    "wasm"
  ],
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "parcel-bundler": "^1.6.0",
    "parcel-plugin-disable-loaders": "^1.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
